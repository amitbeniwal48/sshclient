import paramiko
import os
import sys

ssh = paramiko.SSHClient()
ssh.set_missing_host_key_policy(paramiko.AutoAddPolicy())
pswd = input('Password:')
ssh.connect('1.1.1.1', port=22, username='xyz', password= pswd)
channel = ssh.invoke_shell()

channel_data = str()
host = str()
srcfile = str()

